<% unless @networkterminationpoint.nil? %>
<%- max_tabs = 15 -%>
<%- campus_tab = 'tab_15' %>
<script type="text/javascript">
	var initial_campus = '';
</script>
<br/>
    <h1><%= @networkterminationpoint.name %></h1>
	<% if @last_update %>
		<span class="updated_on">
		    (last updated on <%= @networkterminationpoint.updated_at %> by <%= @last_update[:user] %>)
		</span>
	<% end %>
	
	<div id="tabs_pane" class="tabs_pane">
	    <ul class="tab_row">
	        <li id="tab_1" class="selected">
				<span><a href="#" onclick="select_tab(1,<%= max_tabs %>); return false;">Location</a></span>
			</li>
		<% if @networkterminationpoint.is_datacenter? and @current_user.is_equinix_user? %>
		    <li id="tab_14">
			    <span><a href="#" onclick="select_tab(14,<%= max_tabs %>); return false;">Site Info</a></span>
		    </li>
		    <li id="tab_6">
			    <span><a href="#" onclick="select_tab(6,<%= max_tabs %>); return false;">Transportation Access</a></span>
		    </li>
		    <li id="tab_7">
			    <span><a href="#" onclick="select_tab(7,<%= max_tabs %>); return false;">Security</a></span>
		    </li>
		    <li id="tab_8">
			    <span><a href="#" onclick="select_tab(8,<%= max_tabs %>); return false;">Power</a></span>
		    </li>
		    <li id="tab_9">
			    <span><a href="#" onclick="select_tab(9,<%= max_tabs %>); return false;">HVAC</a></span>
		    </li>
		    <li id="tab_10">
			    <span><a href="#" onclick="select_tab(10,<%= max_tabs %>); return false;">Fire</a></span>
		    </li>
		    <li id="tab_12">
			    <span><a href="#" onclick="select_tab(12,<%= max_tabs %>); return false;">Network Services</a></span>
		    </li>
			<li id="tab_11">
				<span><a href="#" onclick="select_tab(11,<%= max_tabs %>); return false;">Managed Services</a></span>
			</li>
			<li id="tab_3">
				<span><a href="#" onclick="select_tab(3,<%= max_tabs %>); return false;">Network Providers</a></span>
			</li>
			<li id="tab_2">
				<span><a href="#" onclick="select_tab(2,<%= max_tabs %>); return false;">Points of Interest</a></span>
			</li>
			<li id="tab_13">
				<span><a href="#" onclick="select_tab(13,<%= max_tabs %>); return false;">Competitive Market</a></span>
			</li>
		<% else %>		
			<li id="tab_3">
				<span><a href="#" onclick="select_tab(3,<%= max_tabs %>); return false;">Network Providers</a></span>
			</li>
			<li id="tab_2">
    			<span><a href="#" onclick="select_tab(2,<%= max_tabs %>); return false;">Points of Interest</a></span>
    		</li>
		<% end %>
			<li id="tab_5">
				<span><a href="#" onclick="select_tab(5,<%= max_tabs %>); return false;">Latency Times</a></span>
			</li>
