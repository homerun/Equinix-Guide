							<div name="editable" style="display: none;">
							    <%= f.check_box :is_future_build, :onclick => "negotiate_hide_show_future_build_date();" %>

								<span id="future_build_dateEdit">
									<b>Estimated Completion Date:</b>
									<%= f.text_field :future_build_date %>
