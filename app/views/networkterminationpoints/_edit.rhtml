									<%= link_to_remote 'Click to Generate', :url => {:controller => 'campus', :action => 'equinix_metro_report_preload', :networkterminationpoint_id => @networkterminationpoint.id, :update_field => "metro_report_link"}, :before => "$('metro_report_link').innerHTML = 'Loading...';", :update => 'preload_div' %>
								<%- else %>
									<%= link_to_remote 'Click to Generate', :url => {:controller => 'campus', :action => 'equinix_metro_report_preload', :market_id => @networkterminationpoint.datacenterdetail.market.id, :update_field => "metro_report_link"}, :before => "$('metro_report_link').innerHTML = 'Loading...';", :update => 'preload_div' %>
								<%- end %>
								<span id="metro_report_link"></span>
							</td>
						</tr>
					<%- end -%>
					<tr>
					    <td></td>
					    <td>
