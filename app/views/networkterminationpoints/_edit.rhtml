							 :edit_page => 'Networkterminationpoint'} %>
			<%- end -%>
			<%- if NEW_CONNECTIONS then -%>
				<div id="np2ntps">
					<%= render :partial => '/np2ntps/np2ntps', :locals => {:networkterminationpoint => @networkterminationpoint, :networkprovider => nil } %>
				</div>
			<%- else -%>
				<div id="networkterminationpoint_networkproviders">
					<%= render :partial => 'network_providers' %>
				</div>
			<%- end -%>
			</div>
		</div>
		<div id="tab_2_content" class="tab_content" style="display: none;">
		    <div id="tab_padding">
			<div id="networkterminationpoint_pois">
				<%= render :partial => 'pois' %>
			</div>
			</div>
		</div>
