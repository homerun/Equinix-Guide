							<div name="editable" style="display: none;">
							    <%= f.select :campus_id, Campus.find(:all).collect{|c| [c.name, c.id]}, {:include_blank => true}, :onchange => "if(this.value == '') $('#{ campus_tab }').style.display = 'none'; else $('#{ campus_tab }').style.display = 'inline'; if($('networkterminationpoint_campus_id').value == initial_campus) { $('campus_initial').style.display = 'block'; $('campus_new').style.display = 'none'; } else {$('campus_initial').style.display = 'none'; $('campus_new').style.display = 'block';}" %>
								<script type="text/javascript">
									var initial_campus = $('networkterminationpoint_campus_id').value;
									if($('networkterminationpoint_campus_id').value=='')
										$('<%= campus_tab %>').style.display = "none";
									else
										$('<%= campus_tab %>').style.display = "inline";
								</script>
							</div>
							<div name="read_only">
								<%= @networkterminationpoint.campus.name unless @networkterminationpoint.campus.nil? %>
