							<div name="read_only">
								<%= @networkterminationpoint.lat %>
							</div>
						</td>
					</tr>
					<tr>
						<td class="edit_field_text">Longitude: </td>
						<td>
							<div name="editable" style="display: none;">
								<%= f.text_field :lon, :value => @networkterminationpoint.lon %>
							</div>
							<div name="read_only">
								<%= @networkterminationpoint.lon %>
							</div>
						</td>
					</tr>
					<tr>
