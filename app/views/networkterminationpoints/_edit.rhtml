							<div name="editable" style="display: none;">
								<%= f.check_box :coordinates_exact %>
							</div>
							<div name="read_only">
								<%= (@networkterminationpoint.coordinates_exact == true)?('Yes'):('No') %>
							</div>
						</td>
					</tr>
					<tr>
						<td class="edit_field_text">NPA-NXX: </td>
						<td>
							<div name="editable" style="display: none;">
								<%= f.text_field :npa_nxx %>
							</div>
							<div name="read_only">
								<%= @networkterminationpoint.npa_nxx %>
							</div>
						</td>
					</tr>
					<tr>
						<td class="edit_field_text">Future Build: </td>
