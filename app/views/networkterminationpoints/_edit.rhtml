							<div name="editable" style="display: none;">
							    <%= f.text_field :state_or_province %>
							</div>
							<div name="read_only">
								<%= @networkterminationpoint.state_or_province %>
							</div>
						</td>
					</tr>
					<tr>
						<td class="edit_field_text">Zipcode: </td>
