            						<input type="button" onclick="toggle_read_only(false); return false;" value="Edit" />
            					</div>
            				<% end %>
            				<div name="make_read_only" style="display:none;">
            					<%= f.submit 'Save Changes'%>
            					<div id="modified" name="modified"><%= render :partial => '/shared/modified' %></div>
            				</div>
					    </td>
					</tr>
				</table>
				
				</div>
